let moveSpeed = 1;
let speedRange = 3;
let posA = 5;
let posB = 5;
function setup() {
  createCanvas(1000, 400);
}

function draw() {
  background(220);
  int(random(10)%2) ? posA+=moveSpeed+random(speedRange) : posB+=moveSpeed+random(speedRange);
  console.log(random(10)%2);
  fill(255,0,0);
  circle(posA, 100, 10)
  
  fill(255,255,0);
  circle(posB, 300, 10)
  
}
